{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "os": "#ACB0BE",
        "closer": "p:os",
        "pink": "#F5BDE6",
        "lavender": "#B7BDF8",
        "blue": "#8AADF4",
        "col1": "#45455B",
        "col2": "#545373",
        "col3": "#63618D",
        "col4": "#716EA4",
        "osicon": "#312f6f",
        "rcol1": "#45475a",
        "rcol2": "#3b3d4f",
        "rcol3": "#313242",
        "rcol4": "#272838"
    },
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "background": "p:col1",
                    "template": "\u00A0"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "background": "p:col2",
                    "template": "\u00A0"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "background": "p:col3",
                    "template": "\u00A0"
                },
                {
                    "foreground": "p:osicon",
                    "background": "p:col4",
                    "template": "  ",
                    "type": "text",
                    "style": "diamond",
                    "trailing_diamond": "\ue0b0"
                },
                {
                    "background": "#5358fd",
                    "foreground": "#ffffff",
                    "properties": {
                        "folder_icon": "..\ue5fe..",
                        "home_icon": "~",
                        "style": "agnoster_short"
                    },
                    "style": "diamond",
                    "trailing_diamond": "\ue0b0",
                    "template": " \uF015 \u00A0{{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "p:pink",
                    "foreground": "#282c34",
                    "properties": {
                        "branch_icon": "\ue725 ",
                        "cherry_pick_icon": "\ue29b ",
                        "commit_icon": "\uf417 ",
                        "fetch_status": false,
                        "fetch_upstream_icon": false,
                        "merge_icon": "\ue727 ",
                        "no_commits_icon": "\uf0c3 ",
                        "rebase_icon": "\ue728 ",
                        "revert_icon": "\uf0e2 ",
                        "tag_icon": "\uf412 "
                    },
                    "trailing_diamond": "\ue0b0",
                    "template": " {{ .HEAD }} ",
                    "style": "diamond",
                    "type": "git"
                },
                {
                    "properties": {
                        "root_icon": "\uf292 "
                    },
                    "style": "diamond",
                    "template": " \uf0e7",
                    "type": "root"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "overflow": "hide",
            "filler": "<#6C7086>.</>",
            "segments": [
                {
                    "background": "p:rcol1",
                    "leading_diamond": "\ue0b2",
                    "properties": {
                        "style": "dallas",
                        "threshold": 0
                    },
                    "style": "diamond",
                    "template": " <#98C379>\uf00c</> <#fff>\udb80\udddd</> <#98C379>{{ .FormattedMs }}s</> ",
                    "type": "executiontime"
                },
                {
                    "background": "p:rcol2",
                    "style": "plain",
                    "template": "\u00A0",
                    "type": "text"
                },
                {
                    "background": "p:rcol3",
                    "style": "plain",
                    "template": "\u00A0",
                    "type": "text"
                },
                {
                    "background": "p:rcol4",
                    "invert_powerline": true,
                    "style": "diamond",
                    "template": "\u00A0",
                    "trailing_diamond": "\u00A0",
                    "type": "text"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#A0A7D3",
                    "style": "plain",
                    "template": "{{ .Name }}",
                    "type": "shell",
                    "properties": {
                        "mapped_shell_names": {
                            "pwsh": "\u276f",
                            "shell": "\u276f",
                            "cmd": ">",
                            "lua": ">",
                            "nu": ":)",
                            "fish": "~>",
                            "zsh": "%",
                            "bash": "$"
                        }
                    }
                }
            ],
            "type": "prompt"
        }
    ],
    "symbolOverrides": {
        "Windows": ""
    },
    "final_space": true,
    "version": 3
}
